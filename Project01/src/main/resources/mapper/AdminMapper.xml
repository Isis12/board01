<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.icia.board.dao.ManagerDao">
	<select id="getMovielist" resultType="Movie">
		SELECT * FROM MOVIE
	</select>
	
	<insert id="mregistration" parameterType="Movie">
		INSERT INTO MOVIE VALUES(M_SEQ.NEXTVAL, #{mo_title}, #{mo_day}, #{mo_perfomer}, #{mo_director}, #{mo_time}, #{mo_genre}, #{mo_country}, #{mo_grade}, #{mo_basic}, DEFAULT, #{mo_photo}, #{mo_orititle})
	</insert>
	
	<select id="getMovieList" resultType="Movie" >
		SELECT * FROM MOVIE
	</select>
</mapper>